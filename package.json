    "lint": "concurrently \"pnpm b lint\" \"pnpm f lint\"",
    "prepare": "husky"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3"
  }
}
