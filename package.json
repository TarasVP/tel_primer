{
  "name": "telegrino",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "b": "pnpm --filter @telegrino/back",
    "f": "pnpm --filter @telegrino/front",
    "dev": "pnpm -r --parallel dev",
