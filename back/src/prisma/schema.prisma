generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id @default(uuid())
  nick      String @unique
  password  String
  createdAt DateTime @default(now())
}

model Category {
  id          String @id @default(uuid())
  name        String
  description String
  text        String
  createdAt   DateTime @default(now())
}